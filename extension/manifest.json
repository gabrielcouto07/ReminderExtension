{
    "manifest_version": 3,
    "name": "My Reminder Extension",
    "version": "1.0",
    "description": "A reminder extension for YouTube 'Watch Later' videos",
    "permissions": ["storage", "activeTab", "scripting"],
    "host_permissions": ["https://www.googleapis.com/"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
      }
    },
    "icons": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  }
  